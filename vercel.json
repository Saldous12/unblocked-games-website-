{
  "version": 2,
  "builds": [
    {
      "src": "proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/search(.*)",
      "dest": "/proxy.js"  // Route '/search' to the proxy.js file
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"  // Serve static files from the 'public' folder
    }
  ]
}
